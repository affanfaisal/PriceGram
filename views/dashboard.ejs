<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Dashboard</title>
    <link rel="icon" href="/assets/img/logo.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet"
        type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic" rel="stylesheet"
        type="text/css" />
    <link href="/css/dashboard.css" rel="stylesheet" />
</head>

<body>
    <%-include("partials/navbar.ejs")%>
        <header class="masthead" style="height: 700px;">
            <div class="container position-relative">
                <div class="row justify-content-center">
                    <div class="col-xl-6">
                        <div class="text-white">
                            <h1 class="mb-5 text-center">We'll find the best for you!</h1>
                            <form method="GET" action="/products" class="form-subscribe" id="contactForm"
                                data-sb-form-api-token="API_TOKEN">
                                <div class="row">
                                    <div class="col">
                                        <input name="q" class="form-control form-control-lg" id="q" placeholder="Search"
                                            style="width:500px;" />
                                        <div class="text-left">
                                            <ul class="list-group" id="r" style="display: none; width:500px">
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r1">
                                                </a>
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r2"></a>
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r3"></a>
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r4"></a>
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r5"></a>
                                                <a class="list-group-item list-group-item-action list-group-item-light"
                                                    id="r6"></a>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <button class="btn btn-primary btn-lg" id="search" type="submit"><i class="bi bi-search"></i></button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <section class="py-5">
            <div class="row">

                <h1 class="text-center" style="color:rgb(3, 29, 134); font-weight:700">Categories</h1>
                <div class="col">
                    <div class="card h-100">
                        <a href="products?category=2" style="text-decoration: none; color: black" class="item-link"
                            style="text-decoration: none; color: black">
                            <img class="card-img-top" src="/assets/img/laptop.jpg">
                            <div class="text-center">Computer & Gaming</div>
                        </a>
                    </div>
                </div>
                <div class="col">
                    <a href="products?category=8" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/keyboard.jpg">
                            <div class="text-center">Keyboards</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=7" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/bg-showcase-3.jpg">
                            <div class="text-center">Graphic Cards</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=0" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/mobile.jpg">
                            <div class="text-center">Mobiles & Tablets</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=1" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/tv.jpg">
                            <div class="text-center">TV & Video</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=6" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/mouse.jpg">
                            <div class="text-center">Mouses</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=4" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/covers.jpg">
                            <div class="text-center">Cases & Covers</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=5" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/kids.jpg">
                            <div class="text-center">Kids</div>
                        </div>
                    </a>
                </div>
                <div class="col">
                    <a href="products?category=3" style="text-decoration: none; color: black" class="item-link"
                        style="text-decoration: none; color: black">
                        <div class="card h-100">
                            <img class="card-img-top" src="/assets/img/speaker.jpg">
                            <div class="text-center">Home Audio & Theatre</div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="container px-4 px-lg-5 mt-5">
                <div class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center">
                    <h1><a href="/products?vendor=0" style="color:rgb(3, 29, 134); font-weight:700; text-decoration:none;">Telemart</a></h1>
                    <div></div>
                    <div></div>
                    <div></div>
                    <%products1.forEach(function(product){ %>
                        <div class="col mb-5">
                            <a class="item-link" href="/item/<%=product.id%>"
                                style="text-decoration: none; color: black">
                                <div class="card h-100">
                                    <img class="card-img-top" src=<%=product.mainImageLink%>
                                    alt="..." />
                                    <div class="card-body p-4">
                                        <div class="text-center">
                                            <h5 class="fw-semibold">
                                                <%=product.title.slice(0,100)%>
                                            </h5>
                                            <%if(product.discountedPrice){%>
                                                <div id="price" class="fs-5 mb-6">
                                                    <span class="text-decoration-line-through">
                                                        Rs. <%=product.price%>
                                                    </span>
                                                    <span class="fs-5" style="color:rgb(249, 78, 16); font-weight:700;">
                                                        &#160 Rs. <%=product.discountedPrice%>
                                                    </span>
                                                    <br>
                                                </div>
                                                <%}else{%>
                                                    <div id="price" class="fs-5 mb-5"
                                                        style="color:rgb(249, 78, 16); font-weight:700;">
                                                        <span class="fs-5">
                                                            Rs. <%=product.price%>
                                                        </span>
                                                    </div>
                                                    <%}%>
                                        </div>
                                    </div>

                                </div>
                            </a>
                        </div>
                        <%});%>
                            <h1><a href="/products?vendor=1" style="color:rgb(3, 29, 134); font-weight:700; text-decoration:none;">Daraz</a></h1>
                            <div></div>
                            <div></div>
                            <div></div>
                            <% products2.forEach(function(product){ %>
                                <div class="col mb-5">
                                    <a href="/item/<%=product.id%>" style="text-decoration: none; color: black"
                                        class="item-link" style="text-decoration: none; color: black">
                                        <div class="card h-100">
                                            <img class="card-img-top" src=<%=product.mainImageLink%>
                                            alt="..." />
                                            <div class="card-body p-4">
                                                <div class="text-center">
                                                    <h5 class="fw-semibold">
                                                        <%=product.title.slice(0,100)%>
                                                    </h5>
                                                    <%if(product.discountedPrice){%>
                                                        <div id="price" class="fs-5 mb-6">
                                                            <span class="text-decoration-line-through">
                                                                Rs. <%=product.price %>
                                                            </span>
                                                            <span class="fs-5"
                                                                style="color:rgb(249, 78, 16); font-weight:700;">
                                                                &#160 Rs. <%=product.discountedPrice%>
                                                            </span>
                                                            <br>
                                                        </div>
                                                        <%}else{%>
                                                            <div id="price" class="fs-5 mb-5"
                                                                style="color:rgb(249, 78, 16); font-weight:700;">
                                                                <span class="fs-5">
                                                                    Rs. <%=product.price%>
                                                                </span>
                                                            </div>
                                                            <%}%>
                                                </div>
                                            </div>

                                        </div>
                                    </a>
                                </div>
                                <%});%>
                                    <h1><a href="/products?vendor=2" style="color:rgb(3, 29, 134); font-weight:700; text-decoration:none;">Amazon</a></h1>
                                    <div></div>
                                    <div></div>
                                    <div></div>
                                    <% products3.forEach(function(product){ %>
                                        <div class="col mb-5">
                                            <a class="item-link" href="/item/<%=product.id%>"
                                                style="text-decoration: none; color: black">
                                                <div class="card h-100">
                                                    <img class="card-img-top" src="<%=product.mainImageLink%>"
                                                        alt="..." />
                                                    <div class="card-body p-4">
                                                        <div class="text-center">
                                                            <h5 class="fw-semibold">
                                                                <%=product.title.slice(0,100)%>
                                                            </h5>
                                                            <span class="fs-4"
                                                                style="color:rgb(250, 117, 69); font-weight:700;">
                                                                Rs. <%=product.price%>
                                                            </span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </a>
                                        </div>
                                        <%});%>
                </div>
            </div>
        </section>
        <%-include("partials/footer.ejs")%>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src="/js/dashboard.js"></script>
</body>

</html>